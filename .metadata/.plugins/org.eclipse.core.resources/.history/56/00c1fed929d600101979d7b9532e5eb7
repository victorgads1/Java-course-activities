
public class primeiro_print {

	public static void main(String[] args) {
		
		int a = 45;
		double x = 12.312;
		
	System.out.println(a);
	System.out.printf("%.2f%n",x);
	System.out.printf("%.3f%n",x);
	System.out.printf("eu preciso de " + a + " reais ");
	System.out.println("o cachorro andou metros", + x  + "e descansou minutos" );

	}

}
